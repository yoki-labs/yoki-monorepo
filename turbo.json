{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**",
                "dist/**"
            ]
        },
        "@yoki/bot#build": {
            "dependsOn": [
                "^@yokilabs/bot#build",
                "^@yokilabs/utils#build",
                "@yoki/bot#generate"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "@tuxedo/bot#build": {
            "dependsOn": [
                "^@yokilabs/bot#build",
                "^@yokilabs/utils#build",
                "@tuxedo/bot#generate"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "generate": {
            "dependsOn": [
                "^generate"
            ],
            "cache": false
        },
        "lint": {}
    }
}