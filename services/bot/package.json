{
    "name": "@yoki/bot",
    "private": true,
    "version": "1.0.0",
    "description": "Yoki BOT service",
    "main": "./dist/index.js",
    "scripts": {
        "build": "npx tsc && npx tsc-alias",
        "start": "node dist/index.js",
        "dev": "yarn add typescript && npx nodemon",
        "migrate:dev": "npx dotenv -e ../../.env -- npx prisma migrate dev --name init",
        "migrate:prod": "npx dotenv -e ../../.env.prod -- npx prisma migrate deploy",
        "migrate:prod:ci": "prisma migrate deploy",
        "generate": "npx dotenv -e ../../.env -- npx prisma generate"
    },
    "devDependencies": {
        "@guildedjs/guilded-api-typings": "2.2.0",
        "@types/common-tags": "^1.8.1",
        "@types/ms": "^0.7.31",
        "@types/node": "^16.11.6",
        "@types/node-fetch": "2.5.12",
        "@types/recursive-readdir": "^2.2.0",
        "dotenv-cli": "^5.1.0",
        "prisma": "^3.11.0",
        "tsc-alias": "^1.6.5",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "@discordjs/collection": "^0.5.0",
        "@guildedjs/embeds": "^1.0.5-selfbot",
        "@guildedjs/rest": "2.3.2",
        "@guildedjs/webhook-client": "0.0.4-dev",
        "@guildedjs/ws": "1.2.1",
        "@node-redis/json": "^1.0.2",
        "@prisma/client": "^3.11.0",
        "common-tags": "^1.8.2",
        "dotenv": "^8.2.0",
        "ioredis": "^5.0.3",
        "ms": "^2.1.3",
        "nanoid": "^3.3.2",
        "nodemon": "^2.0.14",
        "node-fetch": "2.6.6",
        "recursive-readdir": "^2.2.2",
        "redis-json": "^6.0.3",
        "tslib": "^2.0.3"
    },
    "imports": {
        "#functions/*": "./dist/functions/*.js"
    },
    "author": "Nico",
    "license": "UNLICENSED"
}
