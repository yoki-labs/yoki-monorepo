{
	"name": "@yoki/bot",
	"private": true,
	"version": "1.0.0",
	"description": "Yoki BOT service",
	"main": "./dist/index.js",
	"scripts": {
		"build": "npx tsc && npx tsc-alias",
		"start": "node dist/index.js",
		"dev": "npx nodemon",
		"lint": "eslint src/",
		"lint:fix": "yarn lint --fix",
		"migrate:dev": "npx dotenv -e ../../.env -- npx prisma migrate dev --name init",
		"migrate:prod": "npx dotenv -e ../../.env.prod -- npx prisma migrate deploy",
		"migrate:prod:ci": "prisma migrate deploy",
		"prisma:env:prod": "npx dotenv -e ../../.env.prod --",
		"prisma:env:dev": "npx dotenv -e ../../.env --",
		"generate": "npx dotenv -e ../../.env -- npx prisma generate",
		"commands": "ts-node scripts/gen-cmds-list.ts"
	},
	"devDependencies": {
		"@guildedjs/guilded-api-typings": "2.8.9",
		"@types/common-tags": "^1.8.1",
		"@types/ms": "^0.7.31",
		"@types/node": "^16.11.6",
		"@types/node-fetch": "2.5.12",
		"@types/recursive-readdir": "^2.2.0",
		"dotenv-cli": "^5.1.0",
		"prisma": "^4.2.1",
		"tsc-alias": "^1.6.5",
		"typed-emitter": "^2.1.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@amplitude/node": "^1.10.0",
		"@discordjs/collection": "^0.5.0",
		"@prisma/client": "^4.2.1",
		"@yokilabs/captcha-generator": "1.1.1",
		"aws-sdk": "^2.1168.0",
		"common-tags": "^1.8.2",
		"dotenv": "^8.2.0",
		"guilded.js": "0.19.0",
		"ms": "^2.1.3",
		"nanoid": "^3.3.2",
		"node-fetch": "2.6.6",
		"nodemon": "^2.0.14",
		"recursive-readdir": "^2.2.2",
		"tslib": "^2.0.3"
	},
	"prisma": {
		"schema": "../prisma/schema.prisma"
	},
	"imports": {
		"#functions/*": "./dist/functions/*.js"
	},
	"author": "Nico",
	"license": "UNLICENSED"
}