{
    "name": "@yoki/bot",
    "private": true,
    "version": "1.0.0",
    "description": "Yoki BOT service",
    "main": "./dist/index.js",
    "scripts": {
        "build": "npx tsc && npx tsc-alias",
        "start": "node dist/index.js",
        "dev": "npx nodemon",
        "lint": "eslint src/",
        "lint:fix": "yarn lint --fix",
        "migrate:dev": "npx dotenv -e ../../.env -- npx prisma migrate dev --name init",
        "migrate:prod": "npx dotenv -e ../../.env.prod -- npx prisma migrate deploy",
        "migrate:prod:ci": "prisma migrate deploy",
        "generate": "npx dotenv -e ../../.env -- npx prisma generate",
        "commands": "ts-node scripts/gen-cmds-list.ts"
    },
    "workspaces": [
        "../../yokilabs/bot",
        "../../yokilabs/util",
        "../nsfw",
        "../site"
    ],
    "imports": {
        "#functions/*": "./dist/functions/*.js"
    },
    "devDependencies": {
        "@guildedjs/guilded-api-typings": "2.8.2",
        "@types/common-tags": "^1.8.1",
        "@types/ms": "^0.7.31",
        "@types/node": "^16.11.6",
        "@types/node-fetch": "2.5.12",
        "@types/recursive-readdir": "^2.2.0",
        "dotenv-cli": "^5.1.0",
        "prisma": "^4.2.1",
        "tsc-alias": "^1.6.5",
        "typed-emitter": "^2.1.0",
        "typescript": "^4.1.3",
        "eslint": "8.11.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-next": "12.1.6",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "^7.31.7",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "7.0.4",
        "lint-staged": "12.3.7",
        "prettier": "2.6.0",
        "pretty-quick": "3.1.3",
        "rimraf": "3.0.2"
    },
    "dependencies": {
        "@amplitude/node": "^1.10.0",
        "@discordjs/collection": "^0.5.0",
        "@guildedjs/embeds": "^0.2.0",
        "@guildedjs/rest": "2.8.2",
        "@guildedjs/webhook-client": "0.3.7",
        "@guildedjs/ws": "1.4.11",
        "@node-redis/json": "^1.0.2",
        "@prisma/client": "^4.2.1",
        "@yokilabs/bot": "^1.0.0",
        "@yokilabs/captcha-generator": "1.1.1",
        "@yokilabs/util": "^1.0.0",
        "aws-sdk": "^2.1168.0",
        "common-tags": "^1.8.2",
        "dotenv": "^8.2.0",
        "ioredis": "^5.0.3",
        "ms": "^2.1.3",
        "nanoid": "^3.3.2",
        "node-fetch": "2.6.6",
        "nodemon": "^2.0.14",
        "recursive-readdir": "^2.2.2",
        "redis-json": "^6.0.3",
        "tslib": "^2.0.3"
    },
    "author": "Nico",
    "license": "UNLICENSED"
}