{
    "name": "@tuxedo/bot",
    "private": true,
    "version": "1.0.0",
    "description": "Tuxedo bot Docker service",
    "main": "./dist/index.js",
    "author": "Yoki-Labs",
    "license": "PROPRIETARY",
    "scripts": {
        "build": "npx tsc && npx tsc-alias",
        "start": "node dist/index.js",
        "dev": "npx nodemon",
        "lint": "eslint src/",
        "lint:fix": "yarn lint --fix",
        "migrate:dev": "npx dotenv -e ./.env -- npx prisma migrate dev --name init",
        "migrate:prod": "npx dotenv -e ./.env.prod -- npx prisma migrate deploy",
        "migrate:prod:ci": "prisma migrate deploy",
        "generate": "npx dotenv -e ./.env -- npx prisma generate",
        "commands": "ts-node scripts/gen-cmds-list.ts"
    },
    "imports": {
        "#functions/*": "./dist/functions/*.js"
    },
    "workspaces": [
        "../../yokilabs/bot",
        "../../yokilabs/util"
    ],
    "dependencies": {
        "@amplitude/node": "^1.10.2",
        "@discordjs/collection": "^1.2.0",
        "@guildedjs/embeds": "^0.2.0",
        "@guildedjs/rest": "^2.9.1",
        "@guildedjs/webhook-client": "^0.4.2",
        "@guildedjs/ws": "^1.4.11",
        "@node-redis/json": "^1.0.2",
        "common-tags": "^1.8.2",
        "dotenv": "^16.0.3",
        "ioredis": "^5.2.4",
        "nanoid": "^4.0.0",
        "node-fetch": "^3.2.10",
        "nodemon": "^2.0.20",
        "recursive-readdir": "^2.2.3",
        "redis-json": "^6.0.3",
        "tslib": "^2.4.1",
        "@yokilabs/bot": "*",
        "@yokilabs/util": "*"
    },
    "devDependencies": {
        "@guildedjs/guilded-api-typings": "^2.8.7",
        "@prisma/client": "^4.6.1",
        "@types/common-tags": "^1.8.1",
        "@types/node": "^18.11.9",
        "@types/node-fetch": "^2.6.2",
        "dotenv-cli": "^6.0.0",
        "prisma": "^4.6.1",
        "tsc-alias": "^1.7.0",
        "typed-emitter": "^2.1.0",
        "typescript": "^4.8.4"
    }
}
