{
    "name": "@tuxedo/bot",
    "packageManager": "yarn@1.22.19",
    "private": true,
    "version": "1.0.0",
    "description": "Tuxedo bot Docker service",
    "main": "./dist/index.js",
    "author": "Yoki-Labs",
    "license": "PROPRIETARY",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npx tsc && npx tsc-alias",
        "start": "node dist/index.js",
        "dev": "npx nodemon",
        "lint": "eslint src/",
        "lint:fix": "yarn lint --fix",
        "migrate:dev": "npx dotenv -e ../.env -- npx prisma migrate dev --name init",
        "migrate:prod": "npx dotenv -e ../.env.prod -- npx prisma migrate deploy",
        "migrate:prod:ci": "prisma migrate deploy",
        "generate": "npx dotenv -e ../.env -- npx prisma generate",
        "commands": "ts-node scripts/gen-cmds-list.ts"
    },
    "imports": {
        "#functions/*": "./dist/functions/*.js"
    },
    "workspaces": [
        "../../yokilabs/bot",
        "../../yokilabs/util"
    ],
    "dependencies": {
        "@amplitude/node": "^1.10.2",
        "@discordjs/collection": "^1.2.0",
        "@node-redis/json": "^1.0.2",
        "@yokilabs/bot": "1.0.0",
        "@yokilabs/util": "1.0.0",
        "common-tags": "^1.8.2",
        "dotenv": "^16.0.3",
        "guilded.js": "0.20.0",
        "ioredis": "^5.2.4",
        "nanoid": "^4.0.0",
        "node-fetch": "^3.2.10",
        "nodemon": "^2.0.20",
        "recursive-readdir": "^2.2.3",
        "redis-json": "^6.0.3",
        "tslib": "^2.4.1"
    },
    "devDependencies": {
        "@guildedjs/guilded-api-typings": "^2.8.7",
        "@prisma/client": "^4.6.1",
        "@types/common-tags": "^1.8.1",
        "@types/node": "^18.11.9",
        "@types/node-fetch": "^2.6.2",
        "dotenv-cli": "^6.0.0",
        "eslint": "8.11.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-next": "12.1.6",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "^7.31.7",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "7.0.4",
        "lint-staged": "12.3.7",
        "prettier": "2.6.0",
        "pretty-quick": "3.1.3",
        "prisma": "^4.6.1",
        "rimraf": "3.0.2",
        "tsc-alias": "^1.7.0",
        "typed-emitter": "^2.1.0",
        "typescript": "^4.8.4"
    }
}
